# Lean Research Agent Configuration
# Copy this file to .env and fill in your API keys

# Required: OpenAI API key for LLM and web search
OPENAI_API_KEY=your_openai_api_key_here

# Optional: GitHub token for prior art checking
GITHUB_TOKEN=your_github_token_here

# Optional: Tavily API key for fallback web search
TAVILY_API_KEY=your_tavily_api_key_here

# Optional: OpenAI model override (default: gpt-4o)
OPENAI_MODEL=gpt-4o
